{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@hakyung/x-bot-toolkit/*": ["packages/x-bot-toolkit/src/*"]
    },
    "lib": ["ES2020", "esnext"]
  },
  "exclude": [
    "node_modules"
  ]
}